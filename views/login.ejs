      <div class="center">
        <h1>Log In </h1>
        <label id="name1" for="email">Email:</label> <br>
        <input id="email" type="email" name="" required> <br>

        <label id="name2" for="password">Password:</label> <br>
        <input id="password" type="password" name="" required>

        <p id="forgot-pass"><a href="#">Forgot password</a></p>

